  <SQLQuery>
    <![CDATA[SELECT 
       och.OTHER_CHARGES_NUMBER,
       och.OC_HDR_ID,
	   (select c.CUSTOMER_NAME from xxstg_customer c where c.cust_id=och.cust_id) as cust_name_trans,
	   (select pm.property_name from xxpl_property_master pm where pm.property_id=och.PROPERTY_ID) as Property_Name_Trans,
	   (select pb.build_name from xxpl_property_buildings pb where pb.build_id=och.BUILDING_ID) as Building_Name_Trans,
	   (select pu.unit_name from xxpl_property_units pu where pu.unit_id=och.unit_id) as Unit_Name_Trans,      decode(och.STATUS,'DRA','Draft','PEN','Pending','APR','Approved','REJ','Rejected') as STATUS,
       och.OTHERS_TOTAL_RATE,
       och.RECEIPT_TOTAL,
       och.CREATED_BY,
to_date(to_char(och.CREATION_DATE,'dd-mm-yyyy'),'dd-mm-yyyy') as create_date,
       och.Destination_bu,
(select org_name from xxstg_organizations o where o.org_id = och.Destination_bu) as d_org_name
FROM  XXPM_OC_HEADER och
where 
och.oc_flag='DP' and
och.OTHER_CHARGES_NUMBER is not null
ORDER BY och.OC_HDR_ID DESC]]>
  </SQLQuery>
