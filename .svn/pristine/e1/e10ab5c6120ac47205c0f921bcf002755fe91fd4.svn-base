  <Variable
    Name="P_GRP_ID"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.context.current.sessionScope.get('UserGrpId')]]></TransientExpression>
  </Variable>
  <Variable
