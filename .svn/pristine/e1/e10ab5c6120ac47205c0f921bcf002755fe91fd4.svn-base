    Type="java.lang.String"/>
  <Variable
    Name="p_pl_id"
    Kind="viewcriteria"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[select 
 distinct plh.PL_ID,
 plh.PL_NAME,
 plh.ORG_ID,
 pll.PROPERTY_ID,
(select property_name from xxpl_property_master pm where pm.property_id=pll.PROPERTY_ID) as PROPERTY_NAME,
 plh.START_DATE as hdr_start_date,
 plh.END_DATE as hdr_end_date,
 plh.REVISION_NO as hdr_REVISION_NO,
 plh.REVISION_DATE as hdr_revision_date,
 plh.REVISED_BY as hdr_revised_by,
 plh.ACTIVE_YN as hdr_active_yn
from 
xxpm_pl_header plh,xxpm_pl_lines pll
where plh.PL_ID=pll.PL_ID
and
plh.ORG_ID
IN
(select DISTINCT segment1 from XXFND_DATA_COMBINATION where DCOMB_ID IN
(SELECT DCOMB_ID FROM 
XXFND_DATA_GROUP_DETAIL 
where DATA_GROUP_ID=:P_GRP_ID))]]>
  </SQLQuery>
