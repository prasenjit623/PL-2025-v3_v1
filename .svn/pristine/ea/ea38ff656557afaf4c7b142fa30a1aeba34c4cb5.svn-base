  OfferHeaderVO.TERMS,
  OfferHeaderVO.OBJECT_VERSION_NUMBER,
  OfferHeaderVO.USER_GRP_ID,
  OfferHeaderVO.LEASE_AGREEMENT_ID,
  OfferHeaderVO.OFFER_FLAG,
  OfferHeaderVO.PROPERTY_ID,
  OfferHeaderVO.BUILDING_ID,
  OfferHeaderVO.LEASE_TYPE,
  OfferHeaderVO.TAX_CODE,
  OfferHeaderVO.CAR_PARK_LEASE_AGRE_ID,
  Lead.LEAD_NAME,
  Lead.LEAD_NUMBER,
