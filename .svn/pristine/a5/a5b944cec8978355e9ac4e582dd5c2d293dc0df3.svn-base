  <SQLQuery>
    <![CDATA[SELECT
    bh.booking_id,
    bh.booking_number,
    oh.OFFER_NUMBER OFFER_NUMBER,
    XXPM_OFFER_DETAIL.OFFER_DTL_ID OFFER_DTL_ID, 
    XXPM_OFFER_DETAIL.OFFER_HDR_ID OFFER_HDR_ID, 
    XXPM_OFFER_DETAIL.PROPERTY_ID PROPERTY_ID, 
    XXPM_OFFER_DETAIL.BUILDING_ID BUILDING_ID, 
    XXPM_OFFER_DETAIL.UNIT_ID UNIT_ID, 
    XXPM_OFFER_DETAIL.PURPOSE PURPOSE, 
    XXPM_OFFER_DETAIL.UOM UOM, 
    XXPM_OFFER_DETAIL.QUANTITY QUANTITY, 
    XXPM_OFFER_DETAIL.BASE_RATE BASE_RATE, 
    XXPM_OFFER_DETAIL.TOTAL_RATE TOTAL_RATE, 
    XXPM_OFFER_DETAIL.OFFER_AMOUNT OFFER_AMOUNT, 
    XXPM_OFFER_DETAIL.DISCOUNT_AMOUNT DISCOUNT_AMOUNT, 
    XXPM_OFFER_DETAIL.TAX_AMOUNT TAX_AMOUNT, 
    XXPM_OFFER_DETAIL.TAX_CODE TAX_CODE, 
    XXPM_OFFER_DETAIL.TAX_RATE TAX_RATE, 
    XXPM_OFFER_DETAIL.CREATED_BY CREATED_BY, 
    XXPM_OFFER_DETAIL.CREATION_DATE CREATION_DATE, 
    XXPM_OFFER_DETAIL.LAST_UPDATED_BY LAST_UPDATED_BY, 
    XXPM_OFFER_DETAIL.LAST_UPDATE_DATE LAST_UPDATE_DATE, 
    XXPM_OFFER_DETAIL.LAST_UPDATE_LOGIN LAST_UPDATE_LOGIN, 
    XXPM_OFFER_DETAIL.UNIT_TYPE UNIT_TYPE, 
    XXPM_OFFER_DETAIL.AREA_SQ AREA_SQ,
    XXPM_OFFER_DETAIL.ATTRIBUTE3 as Disc_Amt,
    pm.property_name,
    pb.build_name,
    pu.unit_name,
    oh.offer_total,
    ld.lead_name,
    oh.offer_from_date,
    oh.offer_to_date,
    bc.cust_name,
    (select cc.email_id from xxstg_cust_contacts cc where cc.cust_id=bc.cust_id and rownum=1) as email_id
FROM 
    xxpm_booking_header bh,XXPM_OFFER_DETAIL,xxpm_offer_header oh,xxpl_property_master pm,xxpl_property_buildings pb,xxpl_property_units pu,xxpm_lead ld,
    xxpm_booking_customer bc
    WHERE 
    bh.offer_hdr_id=oh.offer_hdr_id
    and bh.booking_id=bc.booking_id(+)
    and oh.OFFER_HDR_ID(+)=XXPM_OFFER_DETAIL.OFFER_HDR_ID
    and XXPM_OFFER_DETAIL.property_id=pm.property_id
    and XXPM_OFFER_DETAIL.building_id=pb.build_id
    and XXPM_OFFER_DETAIL.unit_id=pu.unit_id
    and oh.lead_id=ld.lead_id(+)
    and XXPM_OFFER_DETAIL.unit_id is not null
    and oh.offer_number is not null
    and bh.booking_number is not null]]>
  </SQLQuery>
