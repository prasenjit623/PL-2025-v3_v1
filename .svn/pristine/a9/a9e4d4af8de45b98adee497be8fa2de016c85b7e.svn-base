<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="DateRestrictionsROVO"
  Version="11.1.1.66.73"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.ROVO.DateRestrictionsROVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="currdate"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT  (to_date(:curdate,'yyyy-mm-dd')+365) as TOTAL
FROM dual]]>
  </SQLQuery>
  <ViewAttribute
    Name="Total"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Date"
    ColumnType="DATE"
    AliasName="TOTAL"
    Expression="TOTAL"
    SQLType="TIMESTAMP">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="7"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.PropertyLease_ModelBundle"/>
  </ResourceBundle>
</ViewObject>
