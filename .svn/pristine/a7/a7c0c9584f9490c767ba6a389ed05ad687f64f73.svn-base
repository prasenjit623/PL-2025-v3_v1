                                       inlineStyle="font-weight:bold;"/>
                        <f:facet name="buttonBar">
                          <af:toolbar id="t3">
                            <af:spacer width="210" height="10" id="s25"/>
                            <af:commandButton text="Ok" immediate="true"
                                              id="cb8" action="cancel"
                                              actionListener="#{backingBeanScope.OfferBean.onClickCancel}"/>
                            <af:spacer width="10" height="10" id="s26"/>
                            <af:commandButton text="Cancel" id="cb7"
                                              actionListener="#{backingBeanScope.OfferBean.onClickPopupCancel}"/>
                          </af:toolbar>
                        </f:facet>
                      </af:dialog>
                    </af:popup>
