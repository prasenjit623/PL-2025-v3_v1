                                      partialTriggers="soc2" immediate="true"
                                      actionListener="#{backingBeanScope.OfferBean.onClickSave}"
                                      inlineStyle="margin-right:10.0px;"
                                      visible="#{sessionScope.grantMap.Offer =='F' and bindings.Status.inputValue=='DRA'}">
                      <af:setPropertyListener from="#{sessionScope.userName}"
                                              to="#{bindings.LastUpdatedBy.inputValue}"
                                              type="action"/>
                    </af:commandButton>
                     <af:commandButton text="Save For Discount" id="cb111" partialSubmit="true"
                                      partialTriggers="soc2" immediate="true"
                                      actionListener="#{backingBeanScope.OfferBean.onClickSaveForDiscFlow}"
                                      inlineStyle="margin-right:10.0px;"
                                      visible="#{sessionScope.grantMap.Offer =='F' and bindings.Status.inputValue!='DRA'}">
                      <af:setPropertyListener from="#{sessionScope.userName}"
                                              to="#{bindings.LastUpdatedBy.inputValue}"
                                              type="action"/>
                    </af:commandButton>
                    <af:commandButton text="Approved and Complete" id="cb20"
                                      actionListener="#{backingBeanScope.OfferBean.onclickAppAndComplt}"
                                      visible="#{backingBeanScope.OfferBean.approvalUser and bindings.FlowLevel.inputValue eq '2'}"/>
