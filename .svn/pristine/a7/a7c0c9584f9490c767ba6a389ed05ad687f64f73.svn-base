                                  selectionListener="#{bindings.getApprovalHistoryROVo2.collectionModel.makeCurrent}"
                                  rowSelection="single" id="t8" varStatus="vs"
                                  inlineStyle="width:inherit;"
                                  columnStretching="column:c30">
                          <af:column id="c31" headerText="S.No" width="50"
                                     align="center">
                            <af:outputText value="#{vs.index+1}" id="ot6"/>
                          </af:column>
                          <af:column sortProperty="#{bindings.getApprovalHistoryROVo2.hints.ApproverId.name}"
                                     sortable="false"
                                     headerText="#{bindings.getApprovalHistoryROVo2.hints.ApproverId.label}"
                                     id="c34" width="200">
                            <af:selectOneChoice value="#{row.bindings.ApproverId.inputValue}"
                                                label="#{row.bindings.ApproverId.label}"
                                                required="#{bindings.getApprovalHistoryROVo2.hints.ApproverId.mandatory}"
                                                shortDesc="#{bindings.getApprovalHistoryROVo2.hints.ApproverId.tooltip}"
                                                id="soc30" readOnly="true">
                              <f:selectItems value="#{row.bindings.ApproverId.items}"
                                             id="si32"/>
                            </af:selectOneChoice>
                          </af:column>
                          <af:column sortProperty="#{bindings.getApprovalHistoryROVo2.hints.SubmissionDate.name}"
                                     sortable="false"
                                     headerText="#{bindings.getApprovalHistoryROVo2.hints.SubmissionDate.label}"
                                     id="c28" width="80">
                            <af:inputDate value="#{row.bindings.SubmissionDate.inputValue}"
                                          label="#{bindings.getApprovalHistoryROVo2.hints.SubmissionDate.label}"
                                          required="#{bindings.getApprovalHistoryROVo2.hints.SubmissionDate.mandatory}"
                                          shortDesc="#{bindings.getApprovalHistoryROVo2.hints.SubmissionDate.tooltip}"
                                          id="id7" readOnly="true">
                              <f:validator binding="#{row.bindings.SubmissionDate.validator}"/>
                              <af:convertDateTime pattern="dd-MM-yyyy"/>
                            </af:inputDate>
                          </af:column>
                          <af:column headerText="#{bindings.getApprovalHistoryROVo2.hints.SubmittedByTrans.label}"
                                     id="c36">
                            <af:outputText value="#{row.SubmittedByTrans}"
                                           id="ot8"/>
                          </af:column>
                          <af:column headerText="Approved Date"
                                     id="c82">
                            <af:outputText value="#{row.ActionDate}" id="ot12">
                              <af:convertDateTime pattern="dd-MM-yyyy"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="#{bindings.getApprovalHistoryROVo2.hints.Status.name}"
                                     sortable="false"
                                     headerText="#{bindings.getApprovalHistoryROVo2.hints.Status.label}"
                                     id="c27" width="70">
                            <af:inputText value="#{row.bindings.Status.inputValue}"
                                          label="#{bindings.getApprovalHistoryROVo2.hints.Status.label}"
                                          required="#{bindings.getApprovalHistoryROVo2.hints.Status.mandatory}"
                                          columns="#{bindings.getApprovalHistoryROVo2.hints.Status.displayWidth}"
                                          maximumLength="#{bindings.getApprovalHistoryROVo2.hints.Status.precision}"
                                          shortDesc="#{bindings.getApprovalHistoryROVo2.hints.Status.tooltip}"
                                          id="it40" readOnly="true">
                              <f:validator binding="#{row.bindings.Status.validator}"/>
                            </af:inputText>
                          </af:column>
                          <af:column headerText="#{bindings.getApprovalHistoryROVo2.hints.Description.label}"
                                     id="c30" width="200">
                            <af:outputText value="#{row.Description}" id="ot5"/>
                          </af:column>
                        </af:table>
                      </af:dialog>
                    </af:popup>
                  </af:panelGroupLayout>
                </af:gridCell>
              </af:gridRow>
            </af:panelGridLayout>
            <af:spacer width="10" height="10" id="s5"/>
            <af:separator id="s6" inlineStyle="border-color:#C1C1C1;"
                          rendered="false"/>
            <af:panelBox text="Offer Information" id="pb2" showDisclosure="false">
