                                      inlineStyle="margin-right:10.0px;"
                                      visible="#{sessionScope.grantMap.Offer =='F' and bindings.Status.inputValue=='DRA'}">
                      <af:setPropertyListener from="#{sessionScope.userName}"
                                      to="#{bindings.LastUpdatedBy.inputValue}"
                                      type="action"/>
                    </af:commandButton>
                    <af:commandButton text="Submit" id="cb4" immediate="true"
                                      inlineStyle="margin-right:10.0px;"
                                      visible="#{sessionScope.grantMap.Offer =='F' and bindings.Status.inputValue=='DRA'}"
                                      action="#{backingBeanScope.OfferBean.onClickSubmit}">
                       <af:setPropertyListener from="#{sessionScope.userName}"
                                      to="#{bindings.LastUpdatedBy.inputValue}"
                                      type="action"/>
                    </af:commandButton>
                    <af:commandButton text="Print" id="cb10"
                                      inlineStyle="margin-right:10.0px;"
                                      rendered="false"
                                      visible="#{sessionScope.grantMap.Offer =='F' || sessionScope.grantMap.Offer =='V' and bindings.Status.inputValue!='REJ'}">
                      <af:fileDownloadActionListener contentType="application/pdf"
                                                     filename="Offer #{bindings.OfferNumber.inputValue}.pdf"
                                                     method="#{backingBeanScope.OfferBean.onClickPDFReport}"/>
                    </af:commandButton>
                    <af:commandButton text="Approval Details" id="cb15"
                                      visible="#{sessionScope.grantMap.Offer =='F' and (bindings.Status.inputValue!='DRA') }"
                                      inlineStyle="margin-right:10.0px;">
                      <af:showPopupBehavior popupId="p4" triggerType="action"/>
                    </af:commandButton>
                    <af:commandButton text="Cancel" id="cb2" immediate="true"
                                      action="cancel"
                                      inlineStyle="margin-right:10.0px;">
                      <af:showPopupBehavior popupId="p1" triggerType="action"/>
                    </af:commandButton>
                    <af:goImageLink id="gil1" targetFrame="_blank"
                                    destination="#{sessionScope.addEditOffer1}"
                                    icon="/images/func_print_16_act.png"
                                    inlineStyle="margin-right:10.0px;"
                                    visible="#{sessionScope.grantMap.Offer =='F' || sessionScope.grantMap.Offer =='V' }"
                                    text="Print"
                                    rendered="#{bindings.Status.inputValue eq 'APR' or bindings.Status.inputValue eq 'BO'}"/>                    
                     <af:popup id="p5">
