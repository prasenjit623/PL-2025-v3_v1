                        <af:outputText value="Are you sure ?"
                                       id="ot75"
                                       inlineStyle="font-weight:bold;"/>
                        <f:facet name="buttonBar">
                          <af:toolbar id="t35">
                            <af:spacer width="80" height="10" id="s255"/>
                            <af:commandButton text="Yes" immediate="true"
                                              id="cb85" action="cancel"
                                              actionListener="#{backingBeanScope.OfferBean.doChangeStatusToDraft}"/>
                            <af:spacer width="10" height="10" id="s265"/>
                            <af:commandButton text="No" id="cb75" />
                          </af:toolbar>
                        </f:facet>
                      </af:dialog>
                    </af:popup>
                    <af:popup id="p1"
                              binding="#{backingBeanScope.OfferBean.p1}">
                      <af:dialog id="d1" titleIconSource="/images/warning.png"
                                 type="none"
                                 inlineStyle="width:350.0px; height:108px;"
                                 title="Warning">
                        <af:outputText value="Are you sure want to Cancel this Information ?"
