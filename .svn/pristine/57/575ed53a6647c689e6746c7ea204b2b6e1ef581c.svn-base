  ML.MS_HDR_ID
FROM XXPM_BOOKING_HEADER BK,
  XXPM_OFFER_HEADER OH,
  XXPL_MILESTONES_HDR ML
WHERE OH.OFFER_HDR_ID(+)  =BK.OFFER_HDR_ID
AND OH.MS_HDR_ID       =ML.MS_HDR_ID(+)
AND BK.BOOKING_NUMBER IS NOT NULL]]>
