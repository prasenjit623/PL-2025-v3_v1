<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:f="http://java.sun.com/jsf/core">
  <af:pageTemplate viewId="/oracle/apps/uikit/common/pageTemplates/MainPageTemplate.jspx"
                   id="pt1">
    <f:facet name="pageContents">
      <af:panelGroupLayout id="pgl1" layout="scroll">
        <af:panelGroupLayout id="pgl2" layout="vertical">
          <af:panelGridLayout id="pgl3">
            <af:gridRow marginTop="5px" height="auto" marginBottom="5px"
                        id="gr1">
              <af:gridCell marginStart="5px" width="100%" marginEnd="5px"
                           id="gc1" rendered="true">
                <af:outputText value="Manage Booking" id="ot1"
                               inlineStyle="font-size:large; font-weight:bold"/>
              </af:gridCell>
              <af:gridCell id="gc2" halign="end">
                <af:panelGroupLayout id="pgl4" styleClass="AFStretchWidth"
                                     layout="horizontal">
                    <af:commandButton text="Change to Draft" id="cb198" inlineStyle="margin-right:10px;"
                                      visible="#{(sessionScope.grantMap.Booking =='F' and bindings.Status11.inputValue!='DRA' and  bindings.Status11.inputValue!='PEN' and sessionScope.userId == '15') ? true : (sessionScope.grantMap.Booking =='F' and sessionScope.userId == '100') ? true : false}">
                      <af:showPopupBehavior popupId="p8"/>
                    </af:commandButton>
                   <af:commandButton text="Refresh" id="cb60"
                                    inlineStyle="margin-right:10px;"
                                     visible="#{sessionScope.grantMap.Booking =='F'}">
                    <af:showPopupBehavior popupId="pi5" triggerType="action"/>
                  </af:commandButton>
                  <af:commandButton text="Attachments" id="cb6"
                                    action="attachment"
                                    actionListener="#{BookingProcess.onClickAttachments}"
                                    inlineStyle="margin-right:10px;"/>
                  <af:commandButton text="CheckList" id="cb14"
                                    action="Checklist"
                                    actionListener="#{BookingProcess.onClickCheckList}"
                                    inlineStyle="margin-right:10px;"
                                    partialTriggers="it3 cb7 cb1"
                                    disabled="#{bindings.BookingNumber1.inputValue==null}"
                                    visible="#{sessionScope.grantMap.Booking =='F' and bindings.Status11.inputValue=='DRA'}"/>
                  <af:commandButton text="By pass Approval" id="cb102" immediate="true"
                                    inlineStyle="margin-right:10px;"
                                    actionListener="#{BookingProcess.onInvokeBypassAppr}"
                                    visible="#{(bindings.Status11.inputValue=='PEN' and sessionScope.userId == '15' and bindings.OrgId.inputValue =='300000007801262') or (bindings.Status11.inputValue=='PEN' and sessionScope.userId == '100' and bindings.OrgId.inputValue =='300000007801262') ? true : false}">
                    <af:setPropertyListener from="#{'BYPASS_APPR'}" to="#{pageFlowScope.Action}" type="action"/>
                  </af:commandButton>
                  <af:commandButton text="Approve" id="cb12" immediate="true"
                                    inlineStyle="margin-right:10px;"
                                    actionListener="#{BookingProcess.onInvokeAppOrrejec}"
                                    visible="#{BookingProcess.approvalUser}">
                    <af:setPropertyListener from="#{'Approval'}"
                                            to="#{pageFlowScope.Action}"
                                            type="action"/>
                  </af:commandButton>
                  <af:commandButton text="Reject" id="cb13" immediate="true"
                                    inlineStyle="margin-right:10px;"
                                    visible="#{sessionScope.userId == '15' or sessionScope.userId == '32'}"
                                    actionListener="#{BookingProcess.onInvokeAppOrrejec}"
                                    rendered="#{bindings.Status11.inputValue=='PEN'}">
                    <af:setPropertyListener from="#{'Rejection'}"
                                            to="#{pageFlowScope.Action}"
                                            type="action"/>
                  </af:commandButton>
                  <af:commandButton text="Save" id="cb7" partialSubmit="true"
                                    actionListener="#{BookingProcess.onClickSave}"
                                    inlineStyle="margin-right:10px;"
                                    visible="#{(sessionScope.grantMap.Booking =='F' and bindings.Status11.inputValue=='DRA') ? 'true' :(sessionScope.grantMap.Booking =='F' and sessionScope.UR eq 'ACCCOUNTS') ? 'true' :(sessionScope.grantMap.Booking =='F' and sessionScope.UR eq 'ACCOUNTS_MANAGER') ? 'true' : (sessionScope.grantMap.Booking =='F' and sessionScope.userId == '15') ? 'true' : false}">
                    <af:setPropertyListener from="#{sessionScope.userName}"
                                            to="#{bindings.LastUpdatedBy.inputValue}"
                                            type="action"/>
                  </af:commandButton>
                  <af:commandButton text="Save and Close" id="cb1"
                                    actionListener="#{BookingProcess.onClickSave}"
                                    inlineStyle="margin-right:10px;"
                                    partialSubmit="true"
                                    action="#{BookingProcess.OnClickPageClose}"
                                    visible="#{sessionScope.grantMap.Booking =='F' &amp;&amp; bindings.Status11.inputValue=='DRA' ? 'true' : sessionScope.UR == 'ACCCOUNTS'  &amp;&amp; sessionScope.grantMap.Booking =='F' &amp;&amp;  bindings.Status11.inputValue!='APR' &amp;&amp; bindings.Status11.inputValue!='BO' ? 'true' : 'false' }">
                    <af:setPropertyListener from="#{'Y'}"
                                            to="#{pageFlowScope.Resval}"
                                            type="action"/>
                    <af:setPropertyListener from="#{sessionScope.userName}"
                                            to="#{bindings.LastUpdatedBy.inputValue}"
                                            type="action"/>
                  </af:commandButton> 
                  <af:commandButton text="Submit" id="cb11" immediate="true"
                                    actionListener="#{BookingProcess.onclickSubmit}"
                                    inlineStyle="margin-right:10px;"
                                    action="#{BookingProcess.OnClickPageClose}"
                                    visible="#{sessionScope.grantMap.Booking =='F' &amp;&amp; bindings.Status11.inputValue=='DRA'}">
                    <af:setPropertyListener from="#{sessionScope.userName}"
                                            to="#{bindings.LastUpdatedBy.inputValue}"
                                            type="action"/>
                    <af:setPropertyListener from="#{'Pending'}"
                                            to="#{pageFlowScope.Action}"
                                            type="action"/>
                  </af:commandButton>
                  <af:commandButton text="Approval Details" id="cb19"
                                    visible="#{sessionScope.grantMap.Booking =='F' and (bindings.Status11.inputValue!='DRA') }"
                                    inlineStyle="margin-right:10px;">
                    <af:showPopupBehavior popupId="p3" triggerType="action"/>
                  </af:commandButton>
                  <af:commandButton text="Cancel" id="cb2"
                                    actionListener="#{BookingProcess.onClickClose}"
                                    action="back"
                                    inlineStyle="margin-right:10px;"
                                    immediate="true"/>
                  <af:goImageLink id="gil1"
                                  destination="#{sessionScope.bookingPrint}"
                                  targetFrame="_blank"
                                  visible="#{sessionScope.grantMap.Booking =='F' || sessionScope.grantMap.Booking =='V'}"
                                  icon="/images/func_print_16_act.png"
                                  inlineStyle="margin-right:10px;"
                                  text="Print"
                                  rendered="#{bindings.Status11.inputValue=='APR' or bindings.Status11.inputValue=='BO' or bindings.Status11.inputValue=='PEN'}"/>
                  <af:commandButton text="Print" id="cb15"
                                    inlineStyle="margin-right:10px;"
                                    visible="#{sessionScope.grantMap.Booking =='F' || sessionScope.grantMap.Booking =='V'}"
                                    rendered="false">
                    <af:fileDownloadActionListener contentType="application/pdf"
                                                   filename="Booking form #{bindings.BookingNumber1.inputValue}.pdf"
                                                   method="#{BookingProcess.onClickPDFReport}"/>
                  </af:commandButton>
                  <af:commandButton text="Confirm Booking" id="cb3"
                                    action="#{BookingProcess.onReturn}"
                                    actionListener="#{BookingProcess.onclickBookingConfirm}"
                                    inlineStyle="margin-right:10px;"
                                    visible="#{sessionScope.grantMap.Booking =='N'?'false':'true'}"
                                    rendered="false"/>
                <af:popup id="p8">
                      <af:dialog id="d8" titleIconSource="/images/warning.png"
                                 type="none"
                                 inlineStyle="width:350.0px; height:108px;"
                                 title="Change status to Draft">
                        <af:outputText value="Are you sure ?"
                                       id="ot75"
                                       inlineStyle="font-weight:bold;"/>
                        <f:facet name="buttonBar">
                          <af:toolbar id="t35">
                            <af:spacer width="80" height="10" id="s255"/>
                            <af:commandButton text="Yes" immediate="true"
                                              id="cb85" action="back"
                                              actionListener="#{BookingProcess.doChangeStatusToDraft}"/>
                            <af:spacer width="10" height="10" id="s265"/>
                            <af:commandButton text="No" id="cb75" />
                          </af:toolbar>
                        </f:facet>
                      </af:dialog>
                    </af:popup>
                 <af:popup id="pi5" >
                    <af:dialog id="di5" type="none" title="Refresh">
                      <f:facet name="buttonBar">
                        <af:toolbar id="t115" flex="1">
                          <af:spacer width="80" height="10" id="s115"/>
                          <af:commandButton text="Save" id="commandButton15"
                                            action="back"
                                            actionListener="#{BookingProcess.onClickRefreshToPullOffer}"/>
                          <af:spacer width="10" height="10" id="s1050"/>
                          <af:commandButton text="Cancel" id="commandButton25"/>
                        </af:toolbar>
                      </f:facet>
                    </af:dialog>
                  </af:popup>
                  <af:popup id="p2" binding="#{BookingProcess.popup3}">
                    <af:dialog id="d2" type="none"
                               title="Reason for #{pageFlowScope.Action}">
                      <f:facet name="buttonBar">
                        <af:toolbar id="t11" flex="1">
                          <af:spacer width="220" height="10" id="s11"/>
                          <af:commandButton text="Save" id="commandButton1"
                                            action="back"
                                            actionListener="#{BookingProcess.onclickAppOrRej}"/>
                          <af:spacer width="10" height="10" id="s10"/>
                          <af:commandButton text="Cancel" id="commandButton2"/>
                        </af:toolbar>
                      </f:facet>
                      <af:inputText label="Reason" id="inputText1" rows="5"
                                    binding="#{BookingProcess.reason}"
                                    columns="50" autoSubmit="true"/>
                    </af:dialog>
                  </af:popup>
                  <af:popup id="p27" binding="#{BookingProcess.popup27}">
                    <af:dialog id="d27" type="none"
                               title="By Pass Approval !!!">
                      <f:facet name="buttonBar">
                        <af:toolbar id="t127" flex="1">
                          <af:spacer width="220" height="10" id="s127"/>
                          <af:commandButton text="Save" id="cbs27"
                                            action="back"
                                            actionListener="#{BookingProcess.onClickApprove}"/>
                          <af:spacer width="10" height="10" id="s1027"/>
                          <af:commandButton text="Cancel" id="cbc27"/>
                        </af:toolbar>
                      </f:facet>
                    </af:dialog>
                  </af:popup>
                  <af:popup id="p3">
                    <af:dialog id="d3" inlineStyle="width:80.0%;"
                               title="Approval Work Flow History">
                        <f:facet name="buttonBar">
                    <!--to save to handle global site-->
                        <af:toolbar id="t111" flex="1" visible="#{sessionScope.userId eq '100'}">
                          <af:spacer width="220" height="10" id="s1101"/>
                          <!--save to handle global site-->
                  <af:commandButton text="Ok" id="cb709"
                                             partialSubmit="true"
                                             inlineStyle="margin-right:10px;"
                                             actionListener="#{BookingProcess.doSaveHandleGlobalSite}">
                              </af:commandButton>
                          <af:spacer width="10" height="10" id="s1051"/>
                          <af:commandButton text="Cancel" id="commandButton31"/>
                        </af:toolbar>
                      </f:facet>
                      <af:table value="#{bindings.getApprovalHistoryROVo1.collectionModel}"
                                var="row"
                                rows="#{bindings.getApprovalHistoryROVo1.rangeSize}"
                                emptyText="#{bindings.getApprovalHistoryROVo1.viewable ? 'No data to display.' : 'Access Denied.'}"
                                fetchSize="#{bindings.getApprovalHistoryROVo1.rangeSize}"
                                rowBandingInterval="0"
                                selectedRowKeys="#{bindings.getApprovalHistoryROVo1.collectionModel.selectedRow}"
                                selectionListener="#{bindings.getApprovalHistoryROVo1.collectionModel.makeCurrent}"
                                rowSelection="single" id="t12" varStatus="vs"
                                inlineStyle="width:inherit;">
                        <af:column id="c82" headerText="S.No" width="50"
                                   align="center">
                          <af:outputText value="#{vs.index+1}" id="ot12"/>
                        </af:column>
                        <af:column sortProperty="#{bindings.getApprovalHistoryROVo1.hints.ApproverId.name}"
                                   sortable="false"
                                   headerText="#{bindings.getApprovalHistoryROVo1.hints.ApproverId.label}"
                                   id="c79" width="200">
                          <af:selectOneChoice value="#{row.bindings.ApproverId.inputValue}"
                                              label="#{row.bindings.ApproverId.label}"
                                              required="#{bindings.getApprovalHistoryROVo1.hints.ApproverId.mandatory}"
                                              shortDesc="#{bindings.getApprovalHistoryROVo1.hints.ApproverId.tooltip}"
                                              id="soc33" readOnly="true"
                                              autoSubmit="true">
                            <f:selectItems value="#{row.bindings.ApproverId.items}"
                                           id="si35"/>
                          </af:selectOneChoice>
                        </af:column>
                        <af:column sortProperty="#{bindings.getApprovalHistoryROVo1.hints.SubmissionDate.name}"
                                   sortable="false"
                                   headerText="#{bindings.getApprovalHistoryROVo1.hints.SubmissionDate.label}"
                                   id="c80">
                          <af:inputDate value="#{row.bindings.SubmissionDate.inputValue}"
                                        label="#{bindings.getApprovalHistoryROVo1.hints.SubmissionDate.label}"
                                        required="#{bindings.getApprovalHistoryROVo1.hints.SubmissionDate.mandatory}"
                                        shortDesc="#{bindings.getApprovalHistoryROVo1.hints.SubmissionDate.tooltip}"
                                        id="id14" readOnly="true"
                                        autoSubmit="true">
                            <f:validator binding="#{row.bindings.SubmissionDate.validator}"/>
                            <af:convertDateTime pattern="dd-MM-yyyy"/>
                          </af:inputDate>
                        </af:column>
                        <af:column headerText="#{bindings.getApprovalHistoryROVo1.hints.SubmittedByTrans.label}"
                                   id="c11">
                          <af:outputText value="#{row.SubmittedByTrans}"
                                         id="ot4"/>
                        </af:column>
                        <af:column headerText="Approved Date"
                                   id="c73">
                          <af:outputText value="#{row.ActionDate}" id="ot61">
                            <af:convertDateTime pattern="dd-MM-yyyy"/>
                          </af:outputText>
                        </af:column>
                        <af:column sortProperty="#{bindings.getApprovalHistoryROVo1.hints.Status.name}"
                                   sortable="false"
                                   headerText="#{bindings.getApprovalHistoryROVo1.hints.Status.label}"
                                   id="c78">
                          <af:inputText value="#{row.bindings.Status.inputValue}"
                                        label="#{bindings.getApprovalHistoryROVo1.hints.Status.label}"
                                        required="#{bindings.getApprovalHistoryROVo1.hints.Status.mandatory}"
                                        columns="#{bindings.getApprovalHistoryROVo1.hints.Status.displayWidth}"
                                        maximumLength="#{bindings.getApprovalHistoryROVo1.hints.Status.precision}"
                                        shortDesc="#{bindings.getApprovalHistoryROVo1.hints.Status.tooltip}"
                                        id="it93" readOnly="true">
                            <f:validator binding="#{row.bindings.Status.validator}"/>
                          </af:inputText>
                        </af:column>
                        <af:column sortProperty="#{bindings.getApprovalHistoryROVo1.hints.Description.name}"
                                   sortable="false"
                                   headerText="#{bindings.getApprovalHistoryROVo1.hints.Description.label}"
                                   id="c81" width="200">
                          <af:inputText value="#{row.bindings.Description.inputValue}"
                                        label="#{bindings.getApprovalHistoryROVo1.hints.Description.label}"
                                        required="#{bindings.getApprovalHistoryROVo1.hints.Description.mandatory}"
                                        columns="#{bindings.getApprovalHistoryROVo1.hints.Description.displayWidth}"
                                        maximumLength="#{bindings.getApprovalHistoryROVo1.hints.Description.precision}"
                                        shortDesc="#{bindings.getApprovalHistoryROVo1.hints.Description.tooltip}"
                                        id="it92" readOnly="true">
                            <f:validator binding="#{row.bindings.Description.validator}"/>
                          </af:inputText>
                        </af:column>
                      </af:table>
                    </af:dialog>
                  </af:popup>
                </af:panelGroupLayout>
              </af:gridCell>
            </af:gridRow>
          </af:panelGridLayout>
        </af:panelGroupLayout>
        <af:spacer width="10" height="10" id="s2"/>
        <af:panelBox text="Booking " id="pb1">
          <f:facet name="toolbar"/>
          <af:panelFormLayout id="pfl1" maxColumns="3" rows="6">
            <!--<af:outputText value="#{sessionScope.userName}"
                           id="ot15"/>-->
            <af:inputText value="#{bindings.BookingNumber1.inputValue}"
                          label="#{bindings.BookingNumber1.hints.label}"
                          required="#{bindings.BookingNumber1.hints.mandatory}"
                          columns="#{bindings.BookingNumber1.hints.displayWidth}"
                          maximumLength="#{bindings.BookingNumber1.hints.precision}"
                          shortDesc="#{bindings.BookingNumber1.hints.tooltip}"
                          id="it3" autoSubmit="true"
                          contentStyle="width:200px;font-size:medium; font-weight:bold;color:#1367ae;"
                          readOnly="true"
                          partialTriggers="cb7 offerHdrIdTransId"
                          disabled="#{bindings.BookingNumber1.inputValue!=null}">
              <f:validator binding="#{bindings.BookingNumber1.validator}"/>
            </af:inputText>
            <af:inputListOfValues id="offerNumber_TransId"
                                  popupTitle="Search and Select: #{bindings.OfferNumber_Trans.hints.label}"
                                  value="#{bindings.OfferNumber_Trans.inputValue}"
                                  label="#{bindings.OfferNumber_Trans.hints.label}"
                                  model="#{bindings.OfferNumber_Trans.listOfValuesModel}"
                                  required="#{bindings.OfferNumber_Trans.hints.mandatory}"
                                  columns="#{bindings.OfferNumber_Trans.hints.displayWidth}"
                                  shortDesc="#{bindings.OfferNumber_Trans.hints.tooltip}"
                                  contentStyle="width:180.0px"
                                  autoSubmit="true"
                                  valueChangeListener="#{BookingProcess.onClickOffer}"
                                  binding="#{BookingProcess.offer}"
                                  disabled="#{bindings.Status11.inputValue eq 'APR'|| bindings.Status11.inputValue eq 'PEN'}"
                                  rendered="#{bindings.Status11.inputValue!='DRA'}">
              <f:validator binding="#{bindings.OfferNumber_Trans.validator}"/>
            </af:inputListOfValues>
            <af:inputComboboxListOfValues id="offerHdrIdTransId"
                                          popupTitle="Search and Select: #{bindings.OfferHdrIdTrans.hints.label}"
                                          value="#{bindings.OfferHdrIdTrans.inputValue}"
                                          label="#{bindings.OfferHdrIdTrans.hints.label}"
                                          model="#{bindings.OfferHdrIdTrans.listOfValuesModel}"
                                          required="#{bindings.OfferHdrIdTrans.hints.mandatory}"
                                          columns="#{bindings.OfferHdrIdTrans.hints.displayWidth}"
                                          shortDesc="#{bindings.OfferHdrIdTrans.hints.tooltip}"
                                          autoSubmit="true"
                                          valueChangeListener="#{BookingProcess.onClickOffer}"
                                          showRequired="true"
                                          rendered="#{bindings.Status11.inputValue=='DRA'}"
                                          contentStyle="width:160.0px">
              <f:validator binding="#{bindings.OfferHdrIdTrans.validator}"/>
              <af:autoSuggestBehavior suggestItems="#{bindings.OfferHdrIdTrans.suggestItems}"
                                      maxSuggestedItems="3"/>
            </af:inputComboboxListOfValues>
            <af:inputText value="#{bindings.Customer_Name_Trans.inputValue}"
                          label="#{bindings.Customer_Name_Trans.hints.label}"
                          required="#{bindings.Customer_Name_Trans.hints.mandatory}"
                          columns="#{bindings.Customer_Name_Trans.hints.displayWidth}"
                          maximumLength="#{bindings.Customer_Name_Trans.hints.precision}"
                          shortDesc="#{bindings.Customer_Name_Trans.hints.tooltip}"
                          id="it5" partialTriggers="offerNumber_TransId offerHdrIdTransId" autoSubmit="true"
                          contentStyle="width:200.0px" disabled="true">
              <f:validator binding="#{bindings.Customer_Name_Trans.validator}"/>
            </af:inputText>
            <af:inputText value="#{bindings.Customer_Mob_No_Trans.inputValue}"
                          label="#{bindings.Customer_Mob_No_Trans.hints.label}"
                          required="#{bindings.Customer_Mob_No_Trans.hints.mandatory}"
                          columns="#{bindings.Customer_Mob_No_Trans.hints.displayWidth}"
                          maximumLength="#{bindings.Customer_Mob_No_Trans.hints.precision}"
                          shortDesc="#{bindings.Customer_Mob_No_Trans.hints.tooltip}"
                          id="it8" partialTriggers="offerNumber_TransId offerHdrIdTransId" autoSubmit="true"
                          contentStyle="width:200.0px" disabled="true">
              <f:validator binding="#{bindings.Customer_Mob_No_Trans.validator}"/>
            </af:inputText>
            <af:selectOneChoice value="#{bindings.Status11.inputValue}"
                                label="Status"
                                required="#{bindings.Status11.hints.mandatory}"
                                shortDesc="#{bindings.Status11.hints.tooltip}"
                                id="soc1" autoSubmit="true" readOnly="false"
                                disabled="true" contentStyle="width:200.0px">
              <f:selectItems value="#{bindings.Status11.items}" id="si1"/>
            </af:selectOneChoice>
            <af:inputText value="#{bindings.CreatedBy.inputValue}"
                          label="#{bindings.CreatedBy.hints.label}"
                          required="#{bindings.CreatedBy.hints.mandatory}"
                          columns="#{bindings.CreatedBy.hints.displayWidth}"
                          maximumLength="#{bindings.CreatedBy.hints.precision}"
                          shortDesc="#{bindings.CreatedBy.hints.tooltip}"
                          id="it89" disabled="true" autoSubmit="true"
                          contentStyle="width:200.0px" rendered="false">
              <f:validator binding="#{bindings.CreatedBy.validator}"/>
            </af:inputText>
            <af:inputText value="#{bindings.PerformedBy.inputValue}"
                          label="#{bindings.PerformedBy.hints.label}"
                          required="#{bindings.PerformedBy.hints.mandatory}"
                          columns="#{bindings.PerformedBy.hints.displayWidth}"
                          maximumLength="#{bindings.PerformedBy.hints.precision}"
                          shortDesc="#{bindings.PerformedBy.hints.tooltip}"
                          id="it112" autoSubmit="true"
                                disabled="true" contentStyle="width:200.0px">
              <f:validator binding="#{bindings.PerformedBy.validator}"/>
            </af:inputText>
            <af:inputDate value="#{bindings.BookingDate.inputValue}"
                          label="#{bindings.BookingDate.hints.label}"
                          required="#{bindings.BookingDate.hints.mandatory}"
                          shortDesc="#{bindings.BookingDate.hints.tooltip}"
                          id="id1" autoSubmit="true"
                          contentStyle="width:200.0px" partialTriggers="it2"
                          showRequired="true"
                          valueChangeListener="#{BookingProcess.onSelectBookingDate}"
                          disabled="#{sessionScope.userId != '15'}">
              <f:validator binding="#{bindings.BookingDate.validator}"/>
              <af:convertDateTime pattern="dd-MM-yyyy"/>
            </af:inputDate>
            <af:inputDate value="#{bindings.Bookingexpirydate.inputValue}"
                          label="Expiry Date"
                          required="#{bindings.Bookingexpirydate.hints.mandatory}"
                          shortDesc="#{bindings.Bookingexpirydate.hints.tooltip}"
                          id="id9" partialTriggers="id1"
                          contentStyle="width:200.0px" rendered="false">
              <f:validator binding="#{bindings.Bookingexpirydate.validator}"/>
              <af:convertDateTime pattern="dd-MM-yyyy"/>
            </af:inputDate>
            <af:inputText value="#{bindings.PerformedBy.inputValue}"
                          label="#{bindings.PerformedBy.hints.label}"
                          required="#{bindings.PerformedBy.hints.mandatory}"
                          columns="#{bindings.PerformedBy.hints.displayWidth}"
                          maximumLength="#{bindings.PerformedBy.hints.precision}"
                          shortDesc="#{bindings.PerformedBy.hints.tooltip}"
                          id="it114" contentStyle="width:200.0px" readOnly="true"
                          rendered="false">
              <f:validator binding="#{bindings.PerformedBy.validator}"/>
            </af:inputText>
            <af:inputDate value="#{bindings.BookingDueDate.inputValue}"
                          label="#{bindings.BookingDueDate.hints.label}"
                          required="#{bindings.BookingDueDate.hints.mandatory}"
                          shortDesc="#{bindings.BookingDueDate.hints.tooltip}"
                          id="id2" autoSubmit="true"
                          contentStyle="width:200.0px"
                          partialTriggers="it2 pc3:cil3 pc3:t41:cil5"
                          disabled="true"
                          binding="#{BookingProcess.bookingDuedate}">
              <f:validator binding="#{bindings.BookingDueDate.validator}"/>
              <af:convertDateTime pattern="dd-MM-yyyy"/>
            </af:inputDate>
            <af:inputDate value="#{bindings.CustReqMoveinDate.inputValue}"
                          label="#{bindings.CustReqMoveinDate.hints.label}"
                          required="#{bindings.CustReqMoveinDate.hints.mandatory}"
                          shortDesc="#{bindings.CustReqMoveinDate.hints.tooltip}"
                          id="id4"
