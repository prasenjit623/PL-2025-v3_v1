       OfferDetail_EO.PREMIUM_RATE_4, 
       OfferDetail_EO.PREMIUM_RATE_TYPE_4, 
       OfferDetail_EO.PREMIUM_RATE_5, 
       OfferDetail_EO.PREMIUM_RATE_TYPE_5, 
       OfferDetail_EO.DISC_PCT_BASE_RATE, 
       OfferDetail_EO.DISC_BASE_RATE, 
       OfferDetail_EO.DISC_PCT_PREMIUM_RATE_1, 
       OfferDetail_EO.DISC_PREMIUM_RATE_1, 
       OfferDetail_EO.DISC_PCT_PREMIUM_RATE_2, 
       OfferDetail_EO.DISC_PREMIUM_RATE_2, 
       OfferDetail_EO.DISC_PCT_PREMIUM_RATE_3, 
       OfferDetail_EO.DISC_PREMIUM_RATE_3, 
       OfferDetail_EO.DISC_PCT_PREMIUM_RATE_4, 
       OfferDetail_EO.DISC_PREMIUM_RATE_4, 
       OfferDetail_EO.DISC_PCT_PREMIUM_RATE_5, 
       OfferDetail_EO.DISC_PREMIUM_RATE_5, 
       OfferDetail_EO.TOTAL_RATE, 
       OfferDetail_EO.OFFER_AMOUNT, 
       OfferDetail_EO.DISCOUNT_AMOUNT, 
       OfferDetail_EO.TAX_AMOUNT, 
       OfferDetail_EO.TAX_CODE, 
       OfferDetail_EO.TAX_RATE, 
       OfferDetail_EO.ATTRIBUTE_CATEGORY, 
       OfferDetail_EO.ATTRIBUTE1, 
       OfferDetail_EO.ATTRIBUTE2, 
       OfferDetail_EO.ATTRIBUTE3, 
       OfferDetail_EO.ATTRIBUTE4, 
       OfferDetail_EO.ATTRIBUTE5, 
       OfferDetail_EO.ATTRIBUTE6, 
       OfferDetail_EO.ATTRIBUTE7, 
       OfferDetail_EO.ATTRIBUTE8, 
       OfferDetail_EO.ATTRIBUTE9, 
       OfferDetail_EO.ATTRIBUTE10, 
       OfferDetail_EO.CREATED_BY, 
       OfferDetail_EO.CREATION_DATE, 
       OfferDetail_EO.LAST_UPDATED_BY, 
       OfferDetail_EO.LAST_UPDATE_DATE, 
       OfferDetail_EO.LAST_UPDATE_LOGIN, 
       OfferDetail_EO.UNIT_TYPE, 
       OfferDetail_EO.AREA_SQ, 
       OfferDetail_EO.PL_ID, 
       OfferDetail_EO.MUNICIPALITY_CHARGES, 
       OfferDetail_EO.ELECTRICITY_CHARGES, 
       OfferDetail_EO.SECURITY_DEPOSITE, 
       OfferDetail_EO.OTHER_GOVNT_CHARGE_NEW, 
       OfferDetail_EO.OTHER_GOVNT_CHARGE_RNEW, 
       OfferDetail_EO.MUNICIPALITY_PERCENTAGE, 
       OfferDetail_EO.EJARI_PAYMENT_CHARGE, 
       OfferDetail_EO.THIRD_PARTY_SERV_CHARGE, 
       OfferDetail_EO.LEASE_UNIT_MAP, 
       OfferDetail_EO.CHECK_BOXE_YN, 
       OfferDetail_EO.PROPERTY_ID AS PROPERTY_ID1,
       nvl((select SECURITY_DEPOSITE from XXPM_OFFER_DETAIL d,xxpm_offer_header o
where 
o.offer_hdr_id=d.offer_hdr_id and
d.unit_id=OfferDetail_EO.unit_id and d.building_id=OfferDetail_EO.building_id 
and o.lead_id=(select oh.lead_id from xxpm_offer_header oh where oh.offer_hdr_id=OfferDetail_EO.offer_hdr_id) and
d.SECURITY_DEPOSITE is not null and rownum=1),'0') as Security_deposit_trans,
OfferDetail_EO.SET_RENT_PER_ANNUM,
OfferDetail_EO.CANCEL_STATUS,
OfferDetail_EO.PRIMARY_UNIT,
OfferDetail_EO.CANCELLED_TAG,
OfferDetail_EO.NET_RENT_WITHOUT_TAX
FROM XXPM_OFFER_DETAIL OfferDetail_EO]]>
  </SQLQuery>
