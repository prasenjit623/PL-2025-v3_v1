  <SQLQuery>
    <![CDATA[select
ls.lease_agreement_id,
ls.lease_number,
ls.lease_start_date,
ls.lease_end_date,
((ls.lease_end_date - ls.lease_start_date)+1) as no_of_days,
ROUND((bd.base_rate/((ls.lease_end_date - ls.lease_start_date)+1)),2) base_rate_one_day,
ls.booking_id,
bh.booking_number,
ls.property_id,
ls.building_id,
bh.offer_hdr_id,
(select lead_id from xxpm_offer_header oh where oh.offer_hdr_id=bh.offer_hdr_id) as lead_id,
(select offer_flag from xxpm_offer_header oh where oh.offer_hdr_id=bh.offer_hdr_id) as offer_flag,
bd.booking_id as booking_id_detail,
bd.unit_id,
bd.base_rate
from
xxpm_lease_agreement ls, xxpm_booking_header bh, xxpm_booking_detail bd
where
bh.booking_id=ls.booking_id
and bh.booking_id=bd.booking_id
order by ls.lease_agreement_id desc]]>
  </SQLQuery>
