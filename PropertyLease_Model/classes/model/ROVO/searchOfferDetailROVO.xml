    XXPM_OFFER_DETAIL.PREMIUM_RATE_TYPE_1 PREMIUM_RATE_TYPE_1, 
    XXPM_OFFER_DETAIL.PREMIUM_RATE_2 PREMIUM_RATE_2, 
    XXPM_OFFER_DETAIL.PREMIUM_RATE_TYPE_2 PREMIUM_RATE_TYPE_2, 
    XXPM_OFFER_DETAIL.PREMIUM_RATE_3 PREMIUM_RATE_3, 
