    XXPM_OFFER_DETAIL.DISC_BASE_RATE DISC_BASE_RATE, 
    XXPM_OFFER_DETAIL.DISC_PCT_PREMIUM_RATE_1 DISC_PCT_PREMIUM_RATE_1, 
    XXPM_OFFER_DETAIL.DISC_PREMIUM_RATE_1 DISC_PREMIUM_RATE_1, 
    XXPM_OFFER_DETAIL.DISC_PCT_PREMIUM_RATE_2 DISC_PCT_PREMIUM_RATE_2, 
