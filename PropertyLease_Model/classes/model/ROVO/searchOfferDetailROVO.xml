    XXPM_OFFER_DETAIL.DISC_PREMIUM_RATE_5 DISC_PREMIUM_RATE_5, 
    XXPM_OFFER_DETAIL.TOTAL_RATE TOTAL_RATE, 
    XXPM_OFFER_DETAIL.OFFER_AMOUNT OFFER_AMOUNT, 
    XXPM_OFFER_DETAIL.DISCOUNT_AMOUNT DISCOUNT_AMOUNT, 
    XXPM_OFFER_DETAIL.TAX_AMOUNT TAX_AMOUNT, 
    XXPM_OFFER_DETAIL.TAX_CODE TAX_CODE, 
    XXPM_OFFER_DETAIL.TAX_RATE TAX_RATE, 
    XXPM_OFFER_DETAIL.CREATED_BY CREATED_BY, 
    XXPM_OFFER_DETAIL.CREATION_DATE CREATION_DATE, 
    XXPM_OFFER_DETAIL.LAST_UPDATED_BY LAST_UPDATED_BY, 
    XXPM_OFFER_DETAIL.LAST_UPDATE_DATE LAST_UPDATE_DATE, 
    XXPM_OFFER_DETAIL.LAST_UPDATE_LOGIN LAST_UPDATE_LOGIN, 
    XXPM_OFFER_DETAIL.UNIT_TYPE UNIT_TYPE, 
    XXPM_OFFER_DETAIL.AREA_SQ AREA_SQ,
    XXPM_OFFER_DETAIL.ATTRIBUTE3 as Disc_Amt,
    pm.property_name,
    pb.build_name,
    pu.unit_name,
    OH.offer_total,
    ld.lead_name,
    OH.offer_from_date,
    OH.offer_to_date
FROM 
    XXPM_OFFER_DETAIL,xxpm_offer_header OH,xxpl_property_master pm,xxpl_property_buildings pb,xxpl_property_units pu,xxpm_lead ld
    WHERE OH.OFFER_HDR_ID(+)=XXPM_OFFER_DETAIL.OFFER_HDR_ID
    and XXPM_OFFER_DETAIL.property_id=pm.property_id
    and XXPM_OFFER_DETAIL.building_id=pb.build_id
    and XXPM_OFFER_DETAIL.unit_id=pu.unit_id
    and OH.lead_id=ld.lead_id(+)
    and XXPM_OFFER_DETAIL.unit_id is not null
    and OH.offer_number is not null]]>
