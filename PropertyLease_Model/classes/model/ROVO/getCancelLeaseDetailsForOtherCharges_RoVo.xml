  <SQLQuery>
    <![CDATA[SELECT XxpmCancellation_EO.CANCEL_ID, 
       XxpmCancellation_EO.NETDUE,
       XxpmCancellation_EO.FUNC_ID, 
       XxpmCancellation_EO.LEASE_AGREEMENT_ID, 
       XxpmCancellation_EO.BOOKING_ID,
       (select offer_number from xxpm_offer_header oh where oh.OFFER_HDR_ID=XxpmCancellation_EO.OFFER_ID and rownum=1) as OFFER_NUMBER, 
       BH.BOOKING_NUMBER,
       BH.PROPERTY_ID,
       BH.BUILDING_ID,
       RH.RECOMMEND_NUMBER,
       LA.LEASE_NUMBER,
       LA.STATUS as Lease_status,
       bc.CUST_ID_AR,
       bd.UNIT_ID,
       (select pm.property_name from xxpl_property_master pm where pm.property_id=BH.PROPERTY_ID ) as PROPERTY_NAME,
       (select pb.build_name from xxpl_property_buildings pb where pb.BUILD_ID=BH.BUILDING_ID ) as BUILD_NAME,
       (select pu.unit_name from xxpl_property_units pu where pu.UNIT_ID=(select unit_id from xxpm_booking_detail bd where bd.BOOKING_ID=BH.BOOKING_ID and rownum=1) ) as UNIT_NAME,
       XxpmCancellation_EO.RECOMMEND_ID, 
       XxpmCancellation_EO.OFFER_ID, 
       XxpmCancellation_EO.ORG_ID, 
(select org_name from xxstg_organizations o where o.org_id=XxpmCancellation_EO.ORG_ID) as ORG_NAME,
       XxpmCancellation_EO.CANCEL_NUMBER, 
       XxpmCancellation_EO.CON_START_DATE, 
       XxpmCancellation_EO.CON_END_DATE, 
       XxpmCancellation_EO.BASE_RATE, 
       XxpmCancellation_EO.TOTAL_RATE, 
       XxpmCancellation_EO.CUSTOMER_NAME, 
       XxpmCancellation_EO.EMIRATED_ID, 
       XxpmCancellation_EO.NATIONALITY, 
       XxpmCancellation_EO.STATUS as CANCEL_STATUS, 
       XxpmCancellation_EO.CANCEL_DATE, 
       XxpmCancellation_EO.DAYS_CONSUMED, 
       XxpmCancellation_EO.DAYS_REMAINING, 
       XxpmCancellation_EO.DAILY_RENTAL_RATE, 
       XxpmCancellation_EO.DAILY_DISCOUNTING_RATE, 
       XxpmCancellation_EO.NOC_BY, 
       XxpmCancellation_EO.NOC_DATE_ENTERED, 
       XxpmCancellation_EO.NOC_DATE_ACTUAL, 
       XxpmCancellation_EO.CHEQUE_CLEARED_AMOUNT, 
       XxpmCancellation_EO.AMOUNT_WAIVED, 
       XxpmCancellation_EO.CURRENCY, 
       XxpmCancellation_EO.OBJECT_VERSION_NUMBER, 
       XxpmCancellation_EO.DESCRIPTION, 
       XxpmCancellation_EO.USER_GRP_ID, 
       XxpmCancellation_EO.FLOW_STATUS, 
       XxpmCancellation_EO.FLOW_LEVEL, 
       XxpmCancellation_EO.FLOW_WITH, 
       XxpmCancellation_EO.USAGE, 
       XxpmCancellation_EO.ATTRIBUTE_CATEGORY, 
       XxpmCancellation_EO.ATTRIBUTE1, 
       XxpmCancellation_EO.ATTRIBUTE2, 
       XxpmCancellation_EO.ATTRIBUTE3, 
       XxpmCancellation_EO.ATTRIBUTE4, 
       XxpmCancellation_EO.ATTRIBUTE5, 
       XxpmCancellation_EO.ATTRIBUTE6, 
       XxpmCancellation_EO.ATTRIBUTE7, 
       XxpmCancellation_EO.ATTRIBUTE8, 
       XxpmCancellation_EO.ATTRIBUTE9, 
       XxpmCancellation_EO.ATTRIBUTE10, 
       XxpmCancellation_EO.CREATED_BY, 
       XxpmCancellation_EO.CREATION_DATE, 
       XxpmCancellation_EO.LAST_UPDATED_BY, 
       XxpmCancellation_EO.LAST_UPDATE_DATE, 
       XxpmCancellation_EO.LAST_UPDATE_LOGIN,
       XxpmCancellation_EO.PROPOSE_FINAL_STATUS
FROM XXPM_CANCELLATION XxpmCancellation_EO,
     xxpm_booking_header BH,
     XXPM_RECOMMEND_HEADER RH,
     XXPM_LEASE_AGREEMENT LA,
     XXPM_BOOKING_CUSTOMER bc,
     XXPM_BOOKING_DETAIL bd
WHERE
     XxpmCancellation_EO.BOOKING_ID=BH.BOOKING_ID
 AND XxpmCancellation_EO.RECOMMEND_ID=RH.RECOMMEND_ID
 AND XxpmCancellation_EO.LEASE_AGREEMENT_ID=LA.LEASE_AGREEMENT_ID
 AND XxpmCancellation_EO.BOOKING_ID=bc.BOOKING_ID
 AND XxpmCancellation_EO.BOOKING_ID=bd.BOOKING_ID
 AND LA.STATUS ='APR'
 AND XxpmCancellation_EO.CANCEL_NUMBER is not null
 ORDER BY XxpmCancellation_EO.CANCEL_NUMBER DESC]]>
  </SQLQuery>
