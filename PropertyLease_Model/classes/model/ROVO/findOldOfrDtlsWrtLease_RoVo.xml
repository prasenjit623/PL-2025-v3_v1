oh.OFFER_HDR_ID,
oh.offer_number,
od.offer_dtl_id as old_offer_dtl_id,
bh.booking_id,
od.base_rate as old_base_rate,
od.total_rate as old_total_rate,
od.attribute3 as old_discount,
od.tax_amount as old_tax_amount,
od.unit_id,
od.offer_hdr_id as old_offer_hdr_id
from
xxpm_offer_header oh,xxpm_lease_agreement la,xxpm_booking_header bh,xxpm_offer_detail od
where
    oh.lease_agreement_id=la.lease_agreement_id
and la.booking_id=bh.booking_id
and bh.offer_hdr_id=od.offer_hdr_id
and od.unit_id is not null
order by oh.offer_hdr_id desc]]>
