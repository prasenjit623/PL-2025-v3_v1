  <SQLQuery>
    <![CDATA[SELECT 
  OfferHeaderVO.OFFER_HDR_ID,
  OfferHeaderVO.LEAD_ID,
  OfferHeaderVO.STATUS,
  Lead.LEAD_NAME,
  Lead.LEAD_NUMBER,
  offerdetailVo.UNIT_ID,
 (select UNIT_NAME from XXPL_PROPERTY_UNITS where UNIT_ID=offerdetailVo.UNIT_ID and rownum=1) as unit_name
FROM XXPM_OFFER_HEADER OfferHeaderVO,XXPM_OFFER_DETAIL offerdetailVo,
  xxpm_lead Lead
WHERE lead.lead_id(+)=OfferHeaderVO.lead_id and OfferHeaderVO.OFFER_HDR_ID=offerdetailVo.OFFER_HDR_ID
and OfferHeaderVO.OFFER_NUMBER is not null 
order by OfferHeaderVO.OFFER_HDR_ID desc]]>
  </SQLQuery>
