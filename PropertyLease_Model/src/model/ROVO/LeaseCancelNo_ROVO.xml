  <SQLQuery>
    <![CDATA[SELECT distinct
  (select o.org_name from xxstg_organizations o where o.org_id=LeaseAgreement_EO.ORG_ID)as business_unit,
  (select xpm.property_number from xxpl_property_master xpm where xpm.property_id = LeaseAgreement_EO.PROPERTY_ID)as propertynumber,
  (select xpm.build_number from xxpl_property_buildings xpm where xpm.build_id = LeaseAgreement_EO.BUILDING_ID)as buildnumber,
  (select listagg((select xpm.unit_number from xxpl_property_units xpm where xpm.unit_id = od.unit_id), ',') within group (order by od.booking_id) as unit_id from xxpm_booking_detail od where od.booking_id = LeaseAgreement_EO.booking_id group by od.booking_id) as unitnumber_trans,(select 'aa' from dual) as unitnumber_dummy,
  (select booking_number from xxpm_booking_header bh where bh.booking_id=LeaseAgreement_EO.BOOKING_ID and rownum=1)BookingNo_trans, 
  (select BOOKING_DATE from xxpm_booking_header bh where bh.booking_id=LeaseAgreement_EO.BOOKING_ID and rownum=1)BookingDate,
  (select recommend_number from xxpm_recommend_header rh where rh.recommend_id=LeaseAgreement_EO.RECOMMEND_ID and rownum=1)RecommendNo_trans,
  (select offer_number from xxpm_offer_header oh where oh.offer_hdr_id=(SELECT OFFER_HDR_ID FROM XXPM_BOOKING_HEADER BS WHERE BOOKING_ID = LeaseAgreement_EO.BOOKING_ID AND rownum = 1) and rownum = 1) OfferNo_trans, 
      LeaseAgreement_EO.ATTRIBUTE1, 
      LeaseAgreement_EO.ATTRIBUTE10, 
      LeaseAgreement_EO.ATTRIBUTE11, 
      LeaseAgreement_EO.ATTRIBUTE12, 
      LeaseAgreement_EO.ATTRIBUTE13, 
      LeaseAgreement_EO.ATTRIBUTE14, 
      LeaseAgreement_EO.ATTRIBUTE15, 
      LeaseAgreement_EO.ATTRIBUTE16, 
      LeaseAgreement_EO.ATTRIBUTE17, 
      LeaseAgreement_EO.ATTRIBUTE18, 
      LeaseAgreement_EO.ATTRIBUTE19, 
      LeaseAgreement_EO.ATTRIBUTE2, 
      LeaseAgreement_EO.ATTRIBUTE20, 
      LeaseAgreement_EO.ATTRIBUTE3, 
      LeaseAgreement_EO.ATTRIBUTE4, 
      LeaseAgreement_EO.ATTRIBUTE5, 
      LeaseAgreement_EO.ATTRIBUTE6, 
      LeaseAgreement_EO.ATTRIBUTE7, 
      LeaseAgreement_EO.ATTRIBUTE8, 
      LeaseAgreement_EO.ATTRIBUTE9, 
      LeaseAgreement_EO.ATTRIBUTE_CATEGORY, 
      LeaseAgreement_EO.BOOKING_ID, 
      (SELECT 
          CUST_NAME 
      FROM 
          XXPM_BOOKING_CUSTOMER BS 
      WHERE 
          LeaseAgreement_EO.BOOKING_ID = BS.BOOKING_ID AND rownum = 1) AS CUSTOMER,
  (SELECT 
          OFFER_HDR_ID 
      FROM 
          XXPM_BOOKING_HEADER BS 
      WHERE 
          BOOKING_ID = LeaseAgreement_EO.BOOKING_ID AND rownum = 1) AS OFFER_HDR_ID,  
      LeaseAgreement_EO.CREATED_BY, 
      LeaseAgreement_EO.CREATION_DATE, 
      LeaseAgreement_EO.DESCRIPTION, 
      LeaseAgreement_EO.FLOW_LEVEL, 
      LeaseAgreement_EO.FLOW_STATUS, 
      LeaseAgreement_EO.FLOW_WITH, 
      LeaseAgreement_EO.FUNC_ID, 
      LeaseAgreement_EO.LAST_UPDATE_DATE, 
      LeaseAgreement_EO.LAST_UPDATE_LOGIN, 
      LeaseAgreement_EO.LAST_UPDATED_BY, 
      LeaseAgreement_EO.LEASE_AGREEMENT_ID, 
      LeaseAgreement_EO.RECOMMEND_ID, 
      LeaseAgreement_EO.STATUS, 
      LeaseAgreement_EO.TENANCY_CON_DOC_NO, 
      LeaseAgreement_EO.LEASE_NUMBER, 
      LeaseAgreement_EO.ORG_ID, 
      LeaseAgreement_EO.PROPERTY_ID, 
      LeaseAgreement_EO.USER_GRP_ID, 
      LeaseAgreement_EO.BUILDING_ID, 
      LeaseAgreement_EO.LEASE_START_DATE, 
      LeaseAgreement_EO.LEASE_END_DATE, 
      LeaseAgreement_EO.TAX_CODE, 
      LeaseAgreement_EO.NO_OF_YEARS, 
      LeaseAgreement_EO.PURPOSE_OF_RENT, 
      LeaseAgreement_EO.RESIDENTIAL_DETAILS,
      LeaseAgreement_EO.CAR_PARK_LEASE_AGRE_ID,
      LeaseAgreement_EO.OFFER_TOTAL,
      LeaseAgreement_EO.TOTAL_IN_WORDS,
      LeaseAgreement_EO.INTEGRATION_RESPONSE,
      c.CANCEL_NUMBER,c.CANCEL_ID,c.PROPOSE_FINAL_STATUS
  FROM XXPM_LEASE_AGREEMENT LeaseAgreement_EO,XXPM_CANCELLATION c
  where LeaseAgreement_EO.LEASE_NUMBER is not null and LeaseAgreement_EO.LEASE_AGREEMENT_ID=c.LEASE_AGREEMENT_ID 
  and c.status='APR' and c.PROPOSE_FINAL_STATUS='Final'
  ORDER BY LEASE_AGREEMENT_ID DESC]]>
  </SQLQuery>
