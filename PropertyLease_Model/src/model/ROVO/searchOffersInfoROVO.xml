  <SQLQuery>
    <![CDATA[SELECT 
  oh.OFFER_HDR_ID ,
  oh.ORG_ID,
  o.org_name as BusinessUnit,
  oh.property_id,
  pm.property_name,
  pb.build_id,
  pb.build_name,
  pm.property_number as propnumber_trans,
(select 'bb' from dual) as propnumber_dummy,
  pb.build_number as buildnumber_trans,
(select 'cc' from dual) as buildnumber_dummy,
(select listagg((select xpm.unit_number from xxpl_property_units xpm where xpm.unit_id = od.unit_id), ',') 
within group (order by od.offer_hdr_id) as
unit_id from xxpm_offer_detail od where od.offer_hdr_id = oh.offer_hdr_id group by od.offer_hdr_id) as unitnumber_trans,
(select 'aa' from dual) as unitnumber_dummy,
pu.unit_name as unitname,
pu.status as unit_status,
pu.unit_id,
pu.description as Unit_description,
od.BASE_RATE as set_rent,
od.TOTAL_RATE as net_rent,
od.TAX_AMOUNT as tax_amount_trans,
od.DISCOUNT_AMOUNT as discount_id,
  oh.FUNC_ID ,
  oh.OFFER_NUMBER ,
  oh.OFFER_DATE ,
  oh.OFFER_FROM_DATE ,
  oh.OFFER_TO_DATE ,
  oh.PERFORMED_BY ,
  oh.REVISION_NO ,
  oh.MS_HDR_ID ,
  oh.OFFER_TYPE ,
  oh.LEAD_ID ,
  ld.LEAD_NUMBER,
  ld.LEAD_NAME,
  ld.MOBILE_NUMBER,
  oh.CUSTOMER_ID ,
  oh.CUST_SITE_ID ,
  oh.SALESMAN_ID ,
  oh.NET_OFFER_AMOUNT ,
  oh.TAX_AMOUNT ,
  oh.STATUS ,
  oh.DESCRIPTION ,
  oh.FLOW_STATUS ,
  oh.FLOW_LEVEL ,
  oh.FLOW_WITH ,
  oh.OFFER_TOTAL,
  oh.CREATED_BY ,
  oh.CREATION_DATE ,
  oh.LAST_UPDATED_BY ,
  oh.LAST_UPDATE_DATE ,
  oh.LAST_UPDATE_LOGIN,
  oh.offer_flag,
  la.lease_number,
  oh.EXTENSION_DAYS,
  od.SET_RENT_PER_ANNUM,
  od.ATTRIBUTE3 as DISC_VALUE,
decode(oh.status,'DRA','Draft','PEN',decode(oh.FLOW_LEVEL,'1','Pending Leasing','2','Pending Legal','3','Pending Landlord'),'APR','Approved','REJ','Rejected','EXP','Expired','CANC','Cancelled','BO','Booked') as sub_status,
oh.UNIT_INCHARGE,oh.UNIT_OWNER
FROM XXPM_OFFER_HEADER oh,xxpm_offer_detail od, XXSTG_ORGANIZATIONS o,xxpl_property_master pm,xxpl_property_buildings pb,xxpl_property_units pu,
XXPM_LEAD ld,xxpm_lease_agreement la
WHERE 
oh.offer_hdr_id=od.offer_hdr_id and
oh.ORG_ID=o.org_id and
oh.property_id=pm.property_id and
oh.building_id=pb.build_id and
od.unit_id=pu.unit_id and
oh.lead_id=ld.lead_id(+) and
oh.lease_agreement_id=la.lease_agreement_id(+) and
oh.ORG_ID
IN
(select DISTINCT segment1 from XXFND_DATA_COMBINATION where DCOMB_ID IN
(SELECT DCOMB_ID FROM 
XXFND_DATA_GROUP_DETAIL 
where DATA_GROUP_ID=:P_GRP_ID)) and
oh.OFFER_NUMBER is not null
ORDER BY "OFFER_HDR_ID" desc]]>
  </SQLQuery>
